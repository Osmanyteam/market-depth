{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "rules": {
        "max-len": ["error", { "code": 100, "tabWidth": 4, "ignoreComments": true }]
    }
